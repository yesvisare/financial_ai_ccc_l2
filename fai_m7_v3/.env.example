# L3 M7.3: Financial Document Parsing & Chunking Configuration

# SEC EDGAR API Configuration (Required for core functionality)
EDGAR_ENABLED=false
# SEC requires User-Agent with company name and email
# Format: "CompanyName product-team@company.com"
# Example: "TechVoyageHub research@techvoyagehub.com"
SEC_USER_AGENT=YourCompany yourteam@company.com

# SEC Rate Limiting (10 requests/second maximum - enforced by SEC)
SEC_RATE_LIMIT=10

# Processing Configuration
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# Optional: OpenAI API (for vector embeddings - enhanced features)
OPENAI_ENABLED=false
OPENAI_API_KEY=sk-your-key-here

# Optional: Pinecone (for vector database storage - enhanced features)
PINECONE_ENABLED=false
PINECONE_API_KEY=your-pinecone-key-here
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=financial-filings

# Logging
LOG_LEVEL=INFO
